<!DOCTYPE html>
<html lang="en">
  <head>
    <%-include('../includes/head.ejs')%>
  </head>
  <header><%-include('../includes/navigation.ejs')%>
  <link rel="stylesheet" href="/css/police/search_results.css"></header>

  <body>
    <nav class="top">

    <div class="search-bar">
    <%-include('../includes/search_bar.ejs')%>
    </div>
    </nav>
    <div>
      <h3 class="title"><%=page_title %></h3>
      <% if(polices.length> 0) { %> <% polices.forEach(p=>{ %>
      <div>
        <img src="/images/polices/<%=p.photo_filename%>" alt="police photo" />
        <span> <%=p.name%> </span>
        <span> <%=p.id %> </span>
        <span> <%=p.department %> </span>
        <a href="/admin/police_personnel_info/<%=p.id%>">More Info</a>
      </div>
      <% }) %> <% } %>
    </div>
  </body>
</html>
