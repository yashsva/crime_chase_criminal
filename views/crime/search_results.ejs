<!DOCTYPE html>
<html lang="en">
  <head>
    <%-include('../includes/head.ejs')%>
  </head>
  <header><%-include('../includes/navigation.ejs')%>
    <link rel="stylesheet" href="/css/police/search_results.css">
  </header>

  <body>
    <nav class="top">

      <div class="search-bar">
    <%-include('../includes/search_bar.ejs')%>
      </div>
      </nav>

    <div>
      <h3 class="title"><%=page_title %></h3>
      <% if(crimes.length> 0) { %> <% crimes.forEach(c=>{ %>
      <div>
        <span> <%=c.id %> </span>
        <span> <%=c.date%> </span>
        <span> <%=c.city%> </span>
        <a href="/crime/crime_detail/<%=c.id%>">More Info</a>
      </div>
      <% }) %> <% } %>
    </div>
  </body>
</html>
